<!DOCTYPE html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <title @text="title"></title>
    <script webc:keep>
      (function () {
        var t = localStorage.getItem('theme') || 'silly';
        document.documentElement.dataset.theme = t;
      })();
    </script>

    <link webc:keep rel="stylesheet" href="/pub/themes/base.css">

    <link webc:keep rel="stylesheet" href="/pub/themes/silly.css" id="theme-silly">
    <link webc:keep rel="stylesheet" href="/pub/themes/srs.css" id="theme-srs" disabled>
  </head>

  <body>
    <main @html="content">
    </main>
    <theme-switcher></theme-switcher>
    <site-footer></site-footer>
  </body>

</html>